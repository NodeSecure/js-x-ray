{
  "name": "@nodesecure/js-x-ray",
  "version": "11.5.0",
  "description": "JavaScript AST XRay analysis",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./warnings": {
      "import": "./dist/warnings.js",
      "types": "./dist/warnings.d.ts"
    },
    "./package.json": "./package.json"
  },
  "engines": {
    "node": ">=24.0.0"
  },
  "scripts": {
    "prepublishOnly": "npm run build",
    "build": "tsc",
    "test-only": "node --test-reporter=spec --test ./test/**/*.spec.ts",
    "test": "c8 --all --src ./src -r html npm run test-only",
    "bench": "node --expose-gc --experimental-strip-types ./benchmark/index.ts"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public",
    "provenance": true
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/NodeSecure/js-x-ray",
    "directory": "workspaces/js-x-ray"
  },
  "keywords": [
    "ast",
    "nsecure",
    "nodesecure",
    "analysis",
    "dependencies",
    "security"
  ],
  "files": [
    "dist"
  ],
  "author": "GENTILHOMME Thomas <gentilhomme.thomas@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/NodeSecure/js-x-ray/issues"
  },
  "homepage": "https://github.com/NodeSecure/js-x-ray#readme",
  "dependencies": {
    "@nodesecure/sec-literal": "^1.4.0",
    "@typescript-eslint/typescript-estree": "8.53.1",
    "digraph-js": "2.2.4",
    "frequency-set": "^2.1.0",
    "ipaddr.js": "2.3.0",
    "meriyah": "7.0.0",
    "safe-regex": "^2.1.1",
    "ts-pattern": "^5.0.6"
  },
  "devDependencies": {
    "@nodesecure/i18n": "4.0.3",
    "astring": "1.9.0",
    "mitata": "1.0.34"
  }
}
